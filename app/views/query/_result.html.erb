<div>
<h3 id="seqresulttag"> Blast results for <%= @query.definition %></h3>
<table class="each-result">

  <tr>
    <th>Number of aligned sequences</th>
    <td>
      <%= @aa_sequence_result.length %>
    </td>
  </tr>
  
  <tr>
    <th>Orthologous Group</th>
    <td>
    	<% if @identity_groups.nil?  %>
    		<%= "No matching orthologous group" %>
    	<% else %>
        <% if @identity_groups.length == 0 %>
          <% if !@existing_matched_group.nil? %>
            <%= @existing_matched_group %>
          <% else %>
            <%= "No matching orthologous group" %>
          <% end %>
        <% else %>

    		  <% @identity_groups.each do |group_number| %>
            <%= "#{group_number}\t" %>
    		  <% end %>
        <% end %>
    	<% end %>
    </td>
  </tr>

  <tr>
    <th>Exist In Database</th>
    <td>
      <% if @is_exist_chain == false %>
        No
      <% else %>
        Yes
      <% end %>
    </td>
  </tr>

</table>
</div>
<hr id="hrtg">
