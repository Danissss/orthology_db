


<div class="container sequence-index">
	<h1>Listing Sequence</h1>
	<hr>
	<table>
	  <thead>
	    <tr>
	      <th colspan="1"></th>
	      <th>Header</th>
	      <th>Group</th>
	      <th>Organism</th>
	      <th>Update</th>
	      
	    </tr>

	    <tr>
	    <%= form_with url:'protein', method: :get do |f| %>
	      <th><%= f.submit "Filter", disabled: false %></th>
	      <th><%= f.text_field 	:header %></th>
	      <th><%= f.text_field  :group %></th>
	      <th><%= f.text_field  :Organism %></th>
	      <th><%= f.date_field  :Update %></th>
	    <% end %>
	      
	    </tr>
	  </thead>
	 	
	  <tbody>
	    <% @protein.each do |pt| %>
	      <tr>
	      	<td><%= link_to "show", protein_path(pt.id) %></td>
	        <td><%= pt.header %></td>
	        <td><%= pt.group %></td>
	        <td><%= pt.organism %></td>
	        <td><%= pt.update_date %></td>
	        
	        
	      </tr>
	    <% end %>
	    
	  </tbody>
	</table>
	<hr>
	<%= paginate @protein %>
	<br>
</div>